{
  "name": "agent-flow",
  "version": "1.0.1",
  "description": "AgentFlow is an intelligent agentic workflow system that manages tasks, dependencies, and reasoning for AI Agents.",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/farazpawle/agent-flow"
  },
  "bugs": {
    "url": "https://github.com/farazpawle/agent-flow/issues"
  },
  "homepage": "https://github.com/farazpawle/agent-flow",
  "bin": {
    "agent-flow": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && copyfiles -E -u 1 \"src/**/*.md\" dist && copyfiles -E -u 1 \"src/public/**/*\" dist && node scripts/add-shebang.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js --gui",
    "gui": "node dist/index.js --gui",
    "mcp": "node dist/index.js",
    "build:start": "npm run build && npm run start",
    "build:mcp": "npm run build && npm run mcp",
    "test": "tsx src/tests/test-rigorous.ts"
  },
  "keywords": [
    "mcp",
    "task-manager",
    "agent",
    "workflow",
    "agent-flow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@supabase/supabase-js": "^2.89.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "get-port": "^7.1.0",
    "minisearch": "^7.2.0",
    "open": "^11.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^20.8.2",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.6",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2"
  }
}